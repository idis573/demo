<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="BbjRecordButton">

        <attr name="backColor" format="color" />        <!--背景圆环颜色-->

        <attr name="progStartColor" format="color" />   <!--进度圆环开始颜色-->
        <attr name="progFirstColor" format="color" />   <!--进度圆环结束颜色-->


        <attr name="progressColor" format="color" />        <!--进度圆环颜色-->
        <attr name="progressWidth" format="dimension" />    <!--背景圆环宽度-->
        <attr name="totalSecond" format="float" />           <!--圆环进度-->
        <attr name="srcPause" format="reference" />         <!--中间按钮资源-->
        <attr name="srcStart" format="reference" />         <!--中间按钮资源-->
        <attr name="blnMaxRadius" format="dimension" />      <!--呼吸效果最大外圈半径-->
        <attr name="blnBackgroundColor" format="color" />      <!--呼吸效果最大外圈半径-->
        <attr name="recordTimeHeight" format="dimension" />
        <attr name="recordTimeMarginBottom" format="dimension" />
        <attr name="recordDotRadius" format="dimension" />
        <attr name="recordDotMarginRight" format="dimension" />
    </declare-styleable>
    <declare-styleable name="CounterFloatView">
        <attr name="padding" format="dimension" />
    </declare-styleable>
</resources>